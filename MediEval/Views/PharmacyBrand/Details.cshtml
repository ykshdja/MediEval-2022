@model PharmacyBrand

@{
    ViewData["Title"] = "Pharmacy Brands";
}

<!doctype html>
<html>
    <head>

    </head>
    <body>
    <div class="row text">
        <div class="col-md-8 offset-2">
            <p>
                <h1>Details for @Model.Name</h1>
            </p>
            <div class="row">
                <form asp-action="Details" asp-controller="PharmacyBrand" method="post">
                    <div class="col-md-8 offset-2">

                        <div class="form-group">
                            <label asp-for="DosageForm" class="control-label"></label>
                            <input asp-for="DosageForm" class="form-control" readonly />
                            <span asp-validation-for="DosageForm" class="text-danger"></span>
                        </div>
                        &nbsp;
                        <div class="form-group">
                            <label asp-for="NDC_Code" class="control-label"></label>
                            <input asp-for="NDC_Code" class="form-control" readonly />
                            <span asp-validation-for="NDC_Code" class="text-danger"></span>
                        </div>
                        &nbsp;
                        <div class="form-group">
                            <label asp-for="category" class="control-label"></label>
                            <input asp-for="category" class="form-control" readonly />
                            <span asp-validation-for="category" class="text-danger"></span>
                        </div>

                        <div class="form-group">
                            <a class="btn btn-outline-secondary" asp-action="Index">Show All</a>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>

    </body>
</html>
