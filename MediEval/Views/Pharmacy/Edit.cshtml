@model Pharmacy

@{
    ViewData["Title"] = "Update Pharmacy";
}



<!DOCTYPE html>

<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title></title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" href="">
    <script src="" async defer></script>
</head>
<body>

    <div class="row text">
        <div class="col-md-8 offset-2">
            <p>
                <h1>Update Pharmacies</h1>
            </p>
            <div class="row">
                <div class="col-md-8 offset-2">
                    <form asp-action="Edit">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>

                        <div class="form-group">
                            <label asp-for="PharmName" class="control-label"></label>
                            <input asp-for="PharmName" class="form-control" />
                            <span asp-validation-for="PharmName" class="text-danger"></span>
                        </div>
                        &nbsp;
                        <div class="form-group">
                            <label asp-for="PharmAddress" class="control-label"></label>
                            <input asp-for="PharmAddress" class="form-control" />
                            <span asp-validation-for="PharmAddress" class="text-danger"></span>
                        </div>
                        &nbsp;
                        <div class="form-group">
                            <label asp-for="PharmPhone" class="control-label"></label>
                            <input asp-for="PharmPhone" class="form-control" />
                            <span asp-validation-for="PharmPhone" class="text-danger"></span>
                        </div>
                        &nbsp;
                        <div class="form-group">
                            <input type="submit" value="Update" class="btn btn-outline-success float-right" />
                            <a class="btn btn-outline-secondary" asp-action="Index">Show All</a>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>

</body>
</html>